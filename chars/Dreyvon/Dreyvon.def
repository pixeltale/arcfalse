; Definition file for Eddy
; Contains all the filenames needed for the character

;Last edit: 09/05/22(Z)

; Player information
[Info]
name = "Dreyvon"        ;Name of character
displayname = "DREYVON" ;Name of character to display
versiondate = 09,30,2009    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "Animated by Dillonator13 | Programmed by Taffy, Z, Bonehouse, and Ace"        ;Character author nam
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 280,240        ;Local coordinate space width and height
ikemenversion=0.99

; Files for the player
[Files]
cmd     = Dreyvon.cmd           ;Command set
cns     = Dreyvon.cns           ;Constants
st      = Dreyvon.cns           ;States
st1      = Normals.cns
st2      = Specials.cns
st3      = HyperCombos.cns
st4      = Target.cns
st5      = Movement.cns
stcommon = common1.cns      ;Common states (from data/ or motif)
sprite  = Dreyvon.sff           ;Sprite
anim    = Dreyvon.air           ;Animation
sound   = Dreyvon.snd           ;Sound
ai      = Dreyvon.ai            ;AI hints data (not used)
movelist=Dreyvon.dat



; Maps character selection buttons to palette numbers.
[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6
x2 = 7 ;Hold Start and press button X for palette 7, etc.
y2 = 8
z2 = 9
a2 = 10
b2 = 11
c2 = 12

