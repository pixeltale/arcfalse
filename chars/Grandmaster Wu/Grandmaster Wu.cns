trigger1 = NumEnemy && fvar(14) 
trigger1 = (EnemyNear, MoveType != H) && (EnemyNear, StateNo != 5300) 
fvar(14) = 0 
ignoreHitPause = 1


[State -2, Dampener] 
type = VarSet 
trigger1 = 1 
fvar(14) = ifElse(fvar(24) < 0, 2, ifElse(fvar(24) >= 14, 1, 2 - fvar(24))) / 2.0
ignoreHitPause = 1 


[State -2, Hit Count Reset] 
