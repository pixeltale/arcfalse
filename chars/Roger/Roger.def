; Definition file for Roger
; Contains all the filenames needed for the character

;Last edit: 08/28/22(Z)
; - Reordered all hurt sprites to fit the recommended group numbers

; Player information
[Info]
name = "Roger"        ;Name of character
displayname = "Roger" ;Name of character to display
versiondate = 09,30,2009    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
ikemenversion	= 0.99
author = "Animated by Dillonator13 | Programmed by Taffy, Kaiba, Z & Bonehouse"         ;Character author nam
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 280,240        ;Local coordinate space width and height


; Files for the player
[Files]
cmd     = Roger.cmd           ;Command set
cns     = Roger.cns           ;Constants
st      = Roger.cns           ;States
st1      = Normals.cns
st2      = Specials.cns
st3      = HyperCombos.cns
st4      = Target.cns
st5      = Helpers.cns
st6		= ROstates.cns
stcommon = common1.cns      ;Common states (from data/ or motif)
sprite  = Roger.sff           ;Sprite
anim    = Roger.air           ;Animation
sound   = Roger.snd           ;Sound
ai      = Roger.ai            ;AI hints data (not used)
movelist 		= Roger.dat
pal2    = 2.act
pal3    = 3.act
pal7 = English.act
pal8 = maoroger.act

; Maps character selection buttons to palette numbers.
[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6
x2 = 7 ;Hold Start and press button X for palette 7, etc.
y2 = 8
z2 = 9
a2 = 10
b2 = 11
c2 = 12
