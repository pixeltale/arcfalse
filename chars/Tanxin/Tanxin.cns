;Night Terror Meter
;=========================================
[State -2, Blood]
type = Helper
trigger1 = !NumHelper(990)&&RoundState=2
stateNo = 990
ID = 990
name = "Night Terror Meter"
posType = left
facing = ifElse(Facing = 1, 1, -1)
