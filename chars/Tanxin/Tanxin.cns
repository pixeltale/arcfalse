[State 0, DisplayToClipboard]
type = DisplayToClipboard
trigger1 = 1
text = "supercancel = %f"
params = var(2)

[State -2, Limiter Reset]	;This resets the Witch Lancer Map so that it can be used again.
type 		= MapSet
trigger1 		= roundstate = 1 || map(BLOOD) > 150
map 			= "BLOOD"
value 		= 150
ignorehitpause 	= 1
[State -2, Limiter Reset]	;This resets the Witch Lancer Map so that it can be used again.
type 		= MapSet
trigger1 		= map(BLOOD) < 0
map 			= "BLOOD"
value 		= 0
ignorehitpause 	= 1
[State -2, Limiter Reset]	;This resets the Witch Lancer Map so that it can be used again.
type 		= MapAdd
trigger1 		= !numhelper(999) && map(Blood) < 150
trigger2		= helper(999), stateno = 998
map 			= "BLOOD"
value 		= 0.75
ignorehitpause 	= 1


[State 0, LifeAdd]
type = PowerAdd
trigger1 = numhelper(999) && helper(999), stateno != 998
value = 1 + (time%3=0) ;Add an extra point every other frame since power values cant be less than 1
ignorehitpause = 1
[State 0, LifeAdd]
type = LifeAdd
trigger1 = numhelper(999) && helper(999), stateno != 998
value = -1
kill = 0
absolute = 0
ignorehitpause = 1
[State -2, Limiter Reset]	;This resets the Witch Lancer Map so that it can be used again.
type 		= MapAdd
trigger1 		= numhelper(999)
map 		= "HealthRefund"
value 		= 1
ignorehitpause 	= 1
[State -2, Limiter Reset]	;This resets the Witch Lancer Map so that it can be used again.
type 		= MapSet
trigger1 		= movetype = H && stateno != [120,155]
map 		= "HealthRefund"
value 		= 0
ignorehitpause 	= 1

[State -2, Limiter Reset]	;This resets the Witch Lancer Map so that it can be used again.
type 		= MapSet
trigger1 		= statetype != A
map 		= "DoubleJump"
value 		= 0
ignorehitpause 	= 1
[State -2, Limiter Reset]	;This resets the Witch Lancer Map so that it can be used again.
type 		= MapSet
trigger1 		= statetype != A
map 		= "Lancer"
value 		= 0
ignorehitpause 	= 1
[State -2, Limiter Reset]	;Reset DF Lancer Limiter
type 		= MapSet		
trigger1 		= !map(ComboStateCheck)
map 			= "DFLancer.HIT"
value 		= 0
ignorehitpause = 1 
