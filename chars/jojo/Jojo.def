;===========================================================================

;Stick Figure Information
[Info]
name = "JoJo"        ;Name of character
displayname = "Jo-Jo (KO)" ;Name of character to display
lifebarname = "KO Jo-Jo"
versiondate = 4,18,2020    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
ikemenversion = 0.99
author = "Programmed by Taffy, Z & Bonehouse"         ;Character author nam
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 280,240        ;Local coordinate space width and height


; Files for the player
