[state 52, Control]
type = ctrlset
trigger1 = !time
trigger1 = !(prevstateno = [600, 699])
trigger2 = time = 1 ; 2 for SNK-style, 1 for Capcom-style. Both are good options.
value = 1

[State 52, Assert: NoWalk]
type = AssertSpecial
trigger1 = Anim != 5
flag = NoWalk
ignoreHitPause = 1

[State 52, ChangeAnim to Turning]
type = ChangeAnim
trigger1 = ctrl && P2Dist x < 0
trigger1 = Anim != 5
value = 5

[State 52, Turn]
type = Turn
trigger1 = Anim = 5 && AnimElem = 1

[State 52, 5]
type = ChangeState
trigger1 = AnimTime = 0
value = 0
ctrl = 1

