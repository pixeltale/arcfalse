menu.itemname.online.serverjoin = "JOIN GAME"               ;Ikemen feature
menu.itemname.online.back = "BACK"                          ;Ikemen feature

; MAIN MENU -> ONLINE -> SERVERJOIN SUBMENU
; known IP addresses show up before serverjoin.back item
