;menu.itemname.extras.sep2 = " - "
menu.itemname.extras.teamarcade = "TEAM ARCADE"
menu.itemname.extras.teamversus = "TEAM VS"
menu.itemname.extras.teamcoop = "TEAM CO-OP"
